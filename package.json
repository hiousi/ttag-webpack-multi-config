{
  "name": "ttag-webpack-multi-config",
  "version": "1.0.0",
  "description": "testing ttga with webpack multi config",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "extract": "webpack --env.extract",
    "build:default": "webpack --env.locale=default",
    "build:fr": "webpack --env.locale=fr",
    "build:es": "webpack --env.locale=es",
    "build:all": "npm run build:default && npm run build:fr && npm run build:es",
    "build:multi": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiousi/ttag-webpack-multi-config.git"
  },
