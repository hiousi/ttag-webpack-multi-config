  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-ttag": "^1.7.8",
    "json-loader": "^0.5.7",
    "po-gettext-loader": "^0.1.1",
    "webpack": "^4.29.6",
